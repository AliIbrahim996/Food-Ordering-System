<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    cardview:cardCornerRadius="4dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backGround"
        android:orientation="vertical"
        android:padding="5dp">

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Full Name"
            cardview:boxCornerRadiusBottomEnd="5dp"
            cardview:boxCornerRadiusBottomStart="5dp"
            cardview:boxCornerRadiusTopEnd="5dp"
            cardview:boxCornerRadiusTopStart="5dp"
            cardview:boxStrokeColor="@color/start_color"
            cardview:boxStrokeWidth="2sp"
            cardview:boxStrokeWidthFocused="@dimen/cardview_compat_inset_shadow"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.0"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toTopOf="parent"
            cardview:layout_constraintVertical_bias="0.022"
            cardview:startIconDrawable="@drawable/usericnof"
            tools:ignore="PrivateResource">

            <EditText
                android:id="@+id/vCName_Value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:textColor="@color/black"
                android:textSize="20dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Gender"
            cardview:boxCornerRadiusBottomEnd="5dp"
            cardview:boxCornerRadiusBottomStart="5dp"
            cardview:boxCornerRadiusTopEnd="5dp"
            cardview:boxCornerRadiusTopStart="5dp"
            cardview:boxStrokeColor="@color/start_color"
            cardview:boxStrokeWidth="2sp"
            cardview:boxStrokeWidthFocused="@dimen/cardview_compat_inset_shadow"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.0"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textInputLayout3"
            cardview:layout_constraintVertical_bias="0.029"
            cardview:startIconDrawable="@drawable/usericnof"
            tools:ignore="PrivateResource">

            <EditText
                android:id="@+id/vCGender_Value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:textColor="@color/black"
                android:textSize="20dp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Location"
            cardview:boxCornerRadiusBottomEnd="5dp"
            cardview:boxCornerRadiusBottomStart="5dp"
            cardview:boxCornerRadiusTopEnd="5dp"
            cardview:boxCornerRadiusTopStart="5dp"
            cardview:boxStrokeColor="@color/start_color"
            cardview:boxStrokeWidth="2sp"
            cardview:boxStrokeWidthFocused="@dimen/cardview_compat_inset_shadow"
            cardview:endIconMode="clear_text"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.0"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textInputLayout5"
            cardview:layout_constraintVertical_bias="0.04"
            cardview:startIconDrawable="@drawable/user_loc"
            tools:ignore="PrivateResource">

            <EditText
                android:id="@+id/vCLoc_Value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:textColor="@color/black"
                android:textSize="20dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Phone Number"
            cardview:boxCornerRadiusBottomEnd="5dp"
            cardview:boxCornerRadiusBottomStart="5dp"
            cardview:boxCornerRadiusTopEnd="5dp"
            cardview:boxCornerRadiusTopStart="5dp"
            cardview:boxStrokeColor="@color/start_color"
            cardview:boxStrokeWidth="2sp"
            cardview:boxStrokeWidthFocused="@dimen/cardview_compat_inset_shadow"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textInputLayout6"
            cardview:layout_constraintVertical_bias="0.047"
            cardview:startIconDrawable="@drawable/user_phone"
            tools:ignore="PrivateResource">

            <EditText
                android:id="@+id/vCPhone_Value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:textColor="@color/black"
                android:textSize="20dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Phone Number"
            cardview:boxCornerRadiusBottomEnd="5dp"
            cardview:boxCornerRadiusBottomStart="5dp"
            cardview:boxCornerRadiusTopEnd="5dp"
            cardview:boxCornerRadiusTopStart="5dp"
            cardview:boxStrokeColor="@color/start_color"
            cardview:boxStrokeWidth="2sp"
            cardview:boxStrokeWidthFocused="@dimen/cardview_compat_inset_shadow"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.0"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textInputLayout8"
            cardview:layout_constraintVertical_bias="0.048"
            cardview:startIconDrawable="@drawable/ic_email_black_24dp"
            tools:ignore="PrivateResource">

            <EditText
                android:id="@+id/Email_Value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:textColor="@color/black"
                android:textSize="20dp" />
        </com.google.android.material.textfield.TextInputLayout>

        <ImageView
            android:id="@+id/vCImage"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:src="@drawable/reservation_manager_48px"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.455"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textView6"
            cardview:layout_constraintVertical_bias="0.145" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Customer-Image"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:textStyle="bold|italic"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintHorizontal_bias="0.45"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/textInputLayout9"
            cardview:layout_constraintVertical_bias="0.198" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>

